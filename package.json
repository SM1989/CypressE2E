{
  "name": "cypresse2e",
  "version": "1.0.0",
  "description": "Test Automation with Cypress",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-prod:addToCartOnly": "npx cypress run --headed --spec cypress/e2e/tests/addToCart.cy.js --config-file prod.config.js",
    "test-prod:registrationOnly": "npx cypress run --headed --spec cypress/e2e/tests/registrationTest.cy.js --config-file prod.config.js",
    "test-prod:regression": "npx cypress run --headed --spec cypress/e2e/tests/*.js --config-file prod.config.js",
    "test-stage:addToCartOnly": "npx cypress run --headed --spec cypress/e2e/tests/addToCart.cy.js --config-file stage.config.js",
    "test-stage:registrationOnly": "npx cypress run --headed --spec cypress/e2e/tests/registrationTest.cy.js --config-file stage.config.js",
    "test-stage:regression": "npx cypress run --headed --spec cypress/e2e/tests/*.js --config-file stage.config.js"
  },
  "repository": {
    "type": "git",
    "url": "(https://github.com/SM1989/CypressE2E.git)"
  },
  "author": "Saurabh Mukherjee",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.4.0",
    "cypress-mochawesome-reporter": "^3.6.1"
  }
}
